{
    "visitReport": {
        "name": "Visit Report",
        "type": "visits",
        "requiredPrivilege": "app:reports",
        "config": {
            "forDataAnalysis": true,
            "patientAttributes": [
                "phoneNumber"
            ],
            "patientAddresses": [
                "City_Village",
                "County_District",
                "State_Province"
            ],
            "visitAttributes": [
                "Visit Status"
            ],
            "excludeColumns": ["Date Of Admission","Date Of Discharge","Patient Id","Visit Id","New patient visit"]
        }
    },
    "OPD/EmergencyVisitCount": {
        "name": "OPD/Emergency Visit Count",
        "type": "MRSGeneric",
        "requiredPrivilege": "app:reports",
        "config": {
            "sqlPath": "/etc/bahmni_config/reports/sql/visitCount.sql"
        }
    }
}
